local function calcField(v1, v2)
    return {
        center = (v1 + v2) / 2,
        bounds = {
            x1 = math.min(v1.X,v2.X), x2 = math.max(v1.X,v2.X),
            z1 = math.min(v1.Z,v2.Z), z2 = math.max(v1.Z,v2.Z),
        }
    }
end

